<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE comps PUBLIC "-//Red Hat, Inc.//DTD Comps info//EN" "comps.dtd">
<comps>

  <group>
    <id>nethserver-sogo</id>
    <_name>SOGo groupware</_name>
    <_description>SOGo server and Thundebird extensions</_description>
    <default>false</default>
    <uservisible>true</uservisible>
    <packagelist>
      <packagereq type="mandatory">nethserver-sogo</packagereq>
    </packagelist>
  </group>

  <group>
    <id>nethserver-virtualbox-5.2-phpvirtualbox</id>
    <_name>phpVirtualBox-5.2</_name>
    <_description>Web user interface for VirtualBox 5.2</_description>
    <default>false</default>
    <uservisible>true</uservisible>
    <packagelist>
      <packagereq type="mandatory">nethserver-virtualbox-5.2-phpvirtualbox</packagereq>
    </packagelist>
  </group>

  <group>
    <id>nethserver-collabora</id>
    <_name>Collabora Online (CODE)</_name>
    <_description>Collabora Online (CODE) integrated with Nextcloud</_description>
    <default>false</default>
    <uservisible>true</uservisible>
    <packagelist>
      <packagereq type="mandatory">nethserver-collabora</packagereq>
    </packagelist>
  </group>
  
  <group>
    <id>nethserver-docker</id>
    <_name>Docker community edition</_name>
    <_description>Docker is an open platform for developing, shipping, and running applications.</_description>
    <default>false</default>
    <uservisible>true</uservisible>
    <packagelist>
      <packagereq type="mandatory">nethserver-docker</packagereq>
    </packagelist>
  </group>
  
  <group>
    <id>nethserver-hotsync</id>
    <_name>HotSync (beta)</_name>
    <_description>Synchronize two NethServer machines using rsync</_description>
    <default>false</default>
    <uservisible>true</uservisible>
    <packagelist>
      <packagereq type="mandatory">nethserver-hotsync</packagereq>
    </packagelist>
  </group>
  
  <group>
    <id>nethserver-mssql</id>
    <_name>Microsoft SQL Server</_name>
    <_description>MSSQL integration for NethServer</_description>
    <default>false</default>
    <uservisible>true</uservisible>
    <packagelist>
      <packagereq type="mandatory">nethserver-mssql</packagereq>
    </packagelist>
  </group>

  <category>
    <id>nethforge</id>
    <_name>NethForge</_name>
    <_description>Modules from the NethServer Community</_description>
    <display_order>1</display_order>
    <grouplist>
        <groupid>nethserver-sogo</groupid>
        <groupid>nethserver-virtualbox-5.2-phpvirtualbox</groupid>
        <groupid>nethserver-collabora</groupid>
        <groupid>nethserver-hotsync</groupid>
        <groupid>nethserver-mssql</groupid>
    </grouplist>
  </category>

</comps>
